   "source": [
    "def upper_lower(s):\n",
    "    u = sum(1 for i in s if i.isupper())\n",
    "    l = sum(1 for i in s if i.islower())\n",
    "    print(\"Sample String :\",s)\n",
    "    print('No. of Upper Characters :',u)\n",
    "    print('No.of Lower Characters :',l)"
   ]
  },
